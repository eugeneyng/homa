{
  "name": "homa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "latest",
    "@fortawesome/free-solid-svg-icons": "latest",
    "@fortawesome/react-fontawesome": "latest",
    "@parse/react": "latest",
    "bulma": "latest",
    "node-sass": "latest",
    "parse": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest",
    "sass": "latest"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/styles.scss src/styles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "react-scripts start && npm run css-watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
